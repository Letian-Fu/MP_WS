<launch>

    <rosparam file="$(find cr5_gazebo)/config/cr5_dh_trajectory_control.yaml" command="load"/>

    <node name="arm_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/cr5" args="arm_joint_controller"/>
    <node name="finger1_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/cr5" args="finger1_joint_controller"/>
    <node name="finger2_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/cr5" args="finger2_joint_controller"/>
</launch>
